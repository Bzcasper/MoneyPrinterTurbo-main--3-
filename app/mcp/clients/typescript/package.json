    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {},
  "files": [
    "dist/**/*",
    "*.ts",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harry0703/MoneyPrinterTurbo.git",
