{
  "name": "@moneyprinter/mcp-client",
  "version": "1.0.0",
  "description": "Vercel-compatible TypeScript/JavaScript client SDK for MoneyPrinterTurbo MCP server",
  "main": "dist/mcp-client-vercel.js",
  "types": "dist/mcp-client-vercel.d.ts",
  "exports": {
    ".": {
      "import": "./dist/mcp-client-vercel.js",
      "require": "./dist/mcp-client-vercel.js",
      "types": "./dist/mcp-client-vercel.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "build:vercel": "tsc --project tsconfig.vercel.json",
