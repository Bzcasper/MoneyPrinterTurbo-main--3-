   "source": [
    "# Test the video generation function\n",
    "async def test_generation():\n",
    "    \"\"\"Test video generation with a simple example.\"\"\"\n",
    "    print(\"🧪 Testing video generation...\")\n",
    "    \n",
    "    # Example parameters\n",
    "    test_params = {\n",
    "        \"video_subject\": \"The future of artificial intelligence\",\n",
    "        \"video_script\": \"\",  # Let AI generate\n",
    "        \"video_terms\": \"\",   # Let AI generate\n",
    "        \"voice_name\": \"en-US-JennyNeural\",\n",
    "        \"video_aspect\": \"9:16\",\n",
    "        \"video_count\": 1,\n",
    "        \"subtitle_enabled\": True\n",
    "    }\n",
    "    \n",
    "    print(f\"📋 Test Parameters:\")\n",
    "    for key, value in test_params.items():\n",
    "        print(f\"   {key}: {value}\")\n",
    "    \n",
    "    print(\"\\n⏳ This would normally take 2-5 minutes on Modal GPU...\")\n",
    "    print(\"\\n🔄 Simulating generation process:\")\n",
    "    \n",
    "    import time\n",
    "    steps = [\n",
