   "source": [
    "@app.function(\n",
    "    image=image,\n",
    "    memory=4096,\n",
    "    volumes={"/mnt/storage\": volume},\n",
    "    allow_concurrent_inputs=50,\n",
    "    keep_warm=1  # Keep one instance warm\n",
    ")\n",
