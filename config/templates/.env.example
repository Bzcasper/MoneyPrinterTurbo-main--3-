# MoneyPrinterTurbo Environment Variables Template
# Copy this file to .env and fill in your actual credentials
# NEVER commit the .env file to version control!

# =============================================================================
# SUPABASE CONFIGURATION
# =============================================================================
# Get these from your Supabase project settings -> API

# Your Supabase project URL
SUPABASE_URL=https://your-project.supabase.co

# Public anon key (safe to expose in client-side code)
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Service role key (server-side only, high privileges)
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Direct database connection (optional)
DATABASE_URL=postgresql://postgres:password@host:port/database

# =============================================================================
# API KEYS - VIDEO SOURCES
# =============================================================================

# Pexels API (get from https://www.pexels.com/api/)
PEXELS_API_KEY=your_pexels_key_here

# Pixabay API (get from https://pixabay.com/api/docs/)
PIXABAY_API_KEY=your_pixabay_key_here

# =============================================================================
# API KEYS - AI/LLM PROVIDERS
# =============================================================================

# OpenAI API (get from https://platform.openai.com/api-keys)
OPENAI_API_KEY=your_openai_key_here
OPENAI_BASE_URL=

# Google Gemini API (get from https://aistudio.google.com/app/apikey)
GEMINI_API_KEY=your_gemini_key_here

# Azure OpenAI Service
AZURE_API_KEY=your_azure_openai_key_here
AZURE_BASE_URL=https://your-resource.openai.azure.com/

# Anthropic Claude API
CLAUDE_API_KEY=your_claude_key_here

# Other LLM Providers
MOONSHOT_API_KEY=your_moonshot_key_here
QWEN_API_KEY=your_qwen_key_here
DEEPSEEK_API_KEY=your_deepseek_key_here
ONEAPI_API_KEY=your_oneapi_key_here

# =============================================================================
# API KEYS - TEXT-TO-SPEECH
# =============================================================================

# Azure Speech Service (get from Azure portal)
AZURE_SPEECH_KEY=your_azure_speech_key_here
AZURE_SPEECH_REGION=eastus

# ElevenLabs TTS
ELEVENLABS_API_KEY=your_elevenlabs_key_here

# GPT-SoVITS (usually local, no key needed)
GPT_SOVITS_API_KEY=

# =============================================================================
# DATABASE CONFIGURATION
# =============================================================================

# PostgreSQL connection details
DB_HOST=localhost
DB_PORT=5432
DB_NAME=moneyprinter
DB_USER=postgres
DB_PASSWORD=your_db_password_here
DB_SSLMODE=prefer

# Database pool settings
DATABASE_POOL_SIZE=10
DATABASE_TIMEOUT=30

# =============================================================================
# REDIS CONFIGURATION
# =============================================================================

# Redis connection (for caching and queues)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=your_redis_password_here

# Alternative: Redis URL format
REDIS_URL=redis://localhost:6379/0

# =============================================================================
# STORAGE CONFIGURATION
# =============================================================================

# AWS S3 Storage
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_BUCKET_NAME=your_s3_bucket_name
AWS_REGION=us-east-1

# Google Cloud Storage
GCS_BUCKET_NAME=your_gcs_bucket_name
GCS_CREDENTIALS_PATH=path/to/service-account.json
GCS_PROJECT_ID=your_gcp_project_id

# Azure Blob Storage
AZURE_STORAGE_ACCOUNT_NAME=your_storage_account_name
AZURE_STORAGE_ACCOUNT_KEY=your_storage_account_key
AZURE_STORAGE_CONTAINER_NAME=your_container_name

# =============================================================================
# SECURITY SETTINGS
# =============================================================================

# JWT Secret (generate with: openssl rand -hex 32)
JWT_SECRET=your_super_secure_jwt_secret_key_here

# Session Secret
SECRET_KEY=your_session_secret_key_here

# MCP HMAC Secrets (for secure API access)
HMAC_CLIENT_001_SECRET=client_001_secret_here
HMAC_CLIENT_002_SECRET=client_002_secret_here

# =============================================================================
# APPLICATION SETTINGS
# =============================================================================

# Environment type
APP_ENV=development
DEPLOYMENT_ENV=development

# Debug settings
DEBUG=true
LOG_LEVEL=INFO

# External service URLs
SERVICE_REGISTRY_URL=http://localhost:8500

# =============================================================================
# MONITORING & LOGGING
# =============================================================================

# Sentry Error Tracking
SENTRY_DSN=your_sentry_dsn_here

# Elasticsearch Logging
ELASTICSEARCH_URL=http://localhost:9200

# =============================================================================
# DOCKER & DEPLOYMENT
# =============================================================================

# Container registry
DOCKER_REGISTRY=your-registry.com
DOCKER_NAMESPACE=moneyprinter
DOCKER_TAG=latest

# TLS/SSL Certificates (for production)
MCP_TLS_CERT_FILE=path/to/cert.pem
MCP_TLS_KEY_FILE=path/to/key.pem
MCP_TLS_CA_FILE=path/to/ca.pem

# =============================================================================
# PROXY SETTINGS (IF NEEDED)
# =============================================================================

# HTTP Proxy settings
HTTP_PROXY=http://proxy:8080
HTTPS_PROXY=http://proxy:8080
NO_PROXY=localhost,127.0.0.1

# =============================================================================
# FEATURE FLAGS
# =============================================================================

# Enable/disable features
ENABLE_EXPERIMENTAL_FEATURES=false
ENABLE_AI_AVATARS=false
ENABLE_BATCH_PROCESSING=true
ENABLE_METRICS=true

# =============================================================================
# NOTES
# =============================================================================

# 1. Never commit this file to version control
# 2. Use strong, unique secrets for all keys
# 3. Regularly rotate API keys and access tokens
# 4. Set appropriate file permissions (600) for this file
# 5. Use a password manager to store sensitive values
# 6. For production, consider using a secrets management service

# Generate secure secrets:
# JWT_SECRET: openssl rand -hex 32
# SESSION_SECRET: openssl rand -hex 32
# HMAC_SECRET: openssl rand -hex 32
